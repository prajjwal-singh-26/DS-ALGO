class Solution {
public:
    int dominantIndex(vector<int>& nums) {
        vector<int>ans=nums;
        int n=ans.size();
        sort(ans.begin(),ans.end());
        if(ans[n-1]>=2*ans[n-2]){
            for(int i=0;i<n;i++){
               if(ans[n-1]==nums[i])
                return i;
            }
        }
        return -1;
    }
};
