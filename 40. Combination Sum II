class Solution {
public:
    void fun(int idx,int target, vector<int>&arr,vector<vector<int>>&ans,vector<int>&cur){
        if(target==0){
            ans.push_back(cur);
            return;
        }
        for(int i=idx;i<arr.size();i++){
            if(i>idx && arr[i]==arr[i-1])continue;
            if(arr[i]>target) break;
            cur.push_back(arr[i]);
            fun(i+1,target-arr[i],arr,ans,cur);
             cur.pop_back();
        }
    }
    vector<vector<int>> combinationSum2(vector<int>& candidates, int target) {
        sort(candidates.begin(), candidates.end());
        vector<vector<int>>ans; 
        vector<int> cur;
        fun(0,target,candidates,ans,cur);
        return ans;
    }
};
